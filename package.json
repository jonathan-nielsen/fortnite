{
    "name": "fortnite",
    "version": "1.0.1",
    "description": "Fortnite",
    "main": "dist/server.js",
    "scripts": {
        "build": "npm run clean && npm run build:server",
        "build:server": "mkdir -p dist/ && babel src/ --out-dir dist/",
        "clean": "rm -rf dist/",
        "dev": "NODE_PATH=src DEBUG=app*,api* nodemon --watch src/ --exec babel-node -- ./src/server.js",
        "postinstall": "npm run build",
        "prestart": "npm install && npm run build",
        "start": "NODE_PATH=dist node dist/app.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jonathan-nielsen/fortnite.git"
    },
    "dependencies": {
        "body-parser": "^1.18.2",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.4",
        "debug": "^3.1.0",
        "dotenv": "^5.0.1",
        "express": "^4.16.3",
        "express-recursive-routes": "^2.0.1",
        "express-session": "^1.15.6",
        "request": "^2.87.0",
        "request-promise": "^4.2.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.24.1",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.5.2",
        "nodemon": "^1.17.3"
    },
    "engines": {
        "node": "8.9.3"
    },
    "author": "Jonathan Nielsen"
}
